on early-init
    write /sys/block/mmcblk0/queue/scheduler noop
	write /sys/block/mmcblk0/queue/read_ahead_kb 2048

on post-fs
    write /dev/cpuset/foreground/cpus 0-7
    write /dev/cpuset/foreground/boost/cpus 0-7
    write /dev/cpuset/background/cpus 0-7
	write /dev/cpuset/system-background/cpus 0-7

    # I/O
    write /sys/block/mmcblk0/queue/rq_affinity 0
    write /sys/block/mmcblk0/queue/add_random 0
    write /sys/block/mmcblk0/bdi/min_ratio 5
	write /sys/block/mmcblk0/queue/read_ahead_kb 128

on property:sys.boot_completed=1
    exec u:r:su:s0 root root -- /gabriel/ext/bbinstall.sh
    exec u:r:su:s0 root root -- /gabriel/ext/init.special_power.sh
    exec u:r:supersu:s0 root root -- /gabriel/ext/bbinstall.sh
    exec u:r:supersu:s0 root root -- /gabriel/ext/init.special_power.sh
