# SPECTRUM KERNEL MANAGER
# Ramdisk file for profile based kernel management
# Implimentation inspired by Franco's fku profiles

# Initialization
on property:sys.boot_completed=1
   # Set default profile on first boot
   exec u:r:init:s0 root root -- /init.spectrum.sh
   exec u:r:su:s0 root root -- /init.spectrum.sh
   # Enable Spectrum support
   setprop spectrum.support 1
   # Add kernel name
   setprop persist.spectrum.kernel Gabriel

# Balance (default profile)
on property:persist.spectrum.profile=0
    write chmod 644 /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor interactive
    write chmod 444 /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor
    write chmod 644 /sys/devices/system/cpu/cpu0/cpufreq/interactive/*
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/use_sched_load 0
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/use_migration_notif 0
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/enable_prediction 0
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay 0
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load 300
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/hispeed_freq 1094400
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads "1 960000:35 1094400:50 1209600:65 1248000:85 1344000:90 1401000:99"
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_rate 40000
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/min_sample_time 0
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/ignore_hispeed_on_notif 1
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/max_freq_hysteresis 0
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_slack -1
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/io_is_busy 0
	write /sys/devices/system/cpu/cpu0/cpufreq/interactive/fast_ramp_down 0
    write chmod 644 /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor interactive
    write chmod 444 /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor
    write chmod 644 /sys/devices/system/cpu/cpu4/cpufreq/interactive/*
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/use_sched_load 0
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/use_migration_notif 1
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/enable_prediction 0
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay 0
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load 300
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/hispeed_freq 998400
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads "1 768000:35 902400:50 998400:80 1094400:100"
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/timer_rate 20000
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/min_sample_time 0
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/ignore_hispeed_on_notif 1
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/max_freq_hysteresis 0
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/timer_slack -1
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/io_is_busy 0
	write /sys/devices/system/cpu/cpu4/cpufreq/interactive/fast_ramp_down 0
	write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 1094400
	write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1401000
	write /sys/module/msm_thermal/core_control/enabled 1
	write /sys/module/msm_thermal/parameters/enabled 0
	write /sys/class/kgsl/kgsl-3d0/devfreq/governor msm-adreno-tz
	write /sys/class/kgsl/kgsl-3d0/devfreq/min_freq 160000000
	write /sys/class/kgsl/kgsl-3d0/devfreq/min_freq 475000000
	write /sys/class/devfreq/1c00000.qcom,kgsl-3d0/device/kgsl/kgsl-3d0/default_pwrlevel 8
	write /sys/module/adreno_idler/parameters/adreno_idler_active 1
	write /sys/module/adreno_idler/parameters/adreno_idler_idlewait 15
	write /sys/module/adreno_idler/parameters/adreno_idler_downdifferential 40
	write /sys/module/adreno_idler/parameters/adreno_idler_idleworkload 5000
	write /sys/block/mmcblk0/queue/scheduler zen
	write /sys/module/workqueue/parameters/power_efficient 1
	write /sys/module/lowmemorykiller/parameters/enable_adaptive_lm 1
	write /proc/sys/net/ipv4/tcp_congestion_control westwood

